windows.debug.x86_64 = "res://addons/godot_lottie/bin/libgodot_lottie.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "res://addons/godot_lottie/bin/libgodot_lottie.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "res://addons/godot_lottie/bin/libgodot_lottie.linux.template_debug.x86_64.so"
